include "../core_timed_defs.csp"

channel AreControlsResponsive



Timed(OneStep) { 
	SelfMessage = timed_priority(Pilot)

	Pilot = AreControlsResponsive -> SKIP 
}

A_Pilot = {AreControlsResponsive, tock}

assert not SelfMessage :[deadlock free]
assert SelfMessage :[divergence free]
assert SelfMessage :[deterministic]
assert SelfMessage :[has trace [T]]: <AreControlsResponsive>
